{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "word-templates-generator": {
      "appLocation": "client",
      "apiLocation": "api",
      "outputLocation": "build",
      "apiLanguage": "dotnet",
      "apiVersion": "6.0",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "dotnet publish -c Release -o bin/publish",
      "run": "npm start",
      "appDevserverUrl": "http://localhost:3000",
      "appName": "word-templates",
      "resourceGroup": "templates-web"
    }
  }
}